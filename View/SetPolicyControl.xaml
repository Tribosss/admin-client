<UserControl x:Class="admin_client.View.SetPolicyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:admin_client.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="160 0 0 0">
        <StackPanel Orientation="Horizontal">
            <Button Width="90" Height="30" Name="AgentOnButton" Click="AgentOnButton_Click" >AGENT ON</Button>
            <Button Width="90" Height="30" Name="AgentOffButton" Click="AgentOffButton_Click" >AGENT OFF</Button>
        </StackPanel>
    </Grid>
</UserControl>
